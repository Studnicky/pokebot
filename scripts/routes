var querystring, url;

url = require('url');
querystring = require('querystring');

module.exports = function(robot) {
  var message;
  robot.router.post('/hubot/notify/:room', function(req, res) {
    var room;
    return room = req.params.room;
  });
  message = req.body.message;
  robot.messageRoom(room, message);
  return res.end();
};